@model AboutResponse
@{
    ViewBag.Title = "About";
}

@Html.RenderMarkdown(ResolveService<IPageTool>().Get("About"))

@section Sidebar
    {
    <partial for="Speakers" name="SpeakersPartial" />
}

@section scripts
{}

@section metadata
    {

    @{
        OpenGraphMetaData openGraphMetaData = ResolveService<OpenGraphMetaData>();
        openGraphMetaData.Title = "Om oss";
        openGraphMetaData.Url = Request.ToFullUrl();
    }
    <partial model="openGraphMetaData" name="MetaDataPartial" />
}