@model AboutResponse
@{
    Layout = "_Framework";

    ViewBag.Title = "Om oss";
}

<vc:markdown name="About"></vc:markdown>

@section metadata
{
    @{
        OpenGraphMetadata metadata = new OpenGraphMetadata {Title = ViewBag.Title};
    }
    <vc:open-graph-metadata data="@metadata"></vc:open-graph-metadata>
}