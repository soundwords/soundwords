@model object

@{
    Type type = Model.GetType();
    IMetaDataTool metaDataTool = ResolveService<IMetaDataTool>();
    Dictionary<string,string> metaData = metaDataTool.GetMetaData(Model);
}

@foreach (var property in metaData.Keys)
{ 
    <meta property="@property" content="@metaData[property]" />
}
